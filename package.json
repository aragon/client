    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.0.1",
    "lint-staged": "^8.1.1",
    "parcel-bundler": "^1.10.1",
